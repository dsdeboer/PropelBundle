{
    "name": "propel/propel-bundle",
    "description": "Integration of Propel in Symfony2",
    "keywords": [
        "propel",
        "orm",
        "persistence"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Duncan de Boer",
            "email": "duncan@charpand.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "Propel\\Bundle\\PropelBundle\\": ""
        },
        "files": [
            "autoload_aliases.php"
        ],
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "config": {
        "platform": {
            "php": "7.0"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": "^7.0, <7.1",
        "dsdeboer/propel1": "1.7.*",
        "symfony/framework-bundle": "^2.8.50",
        "ext-simplexml": "*",
        "ext-pdo": "*"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.5",
        "sensio/framework-extra-bundle": "^3.0.2",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/symfony": "^2.8.2"
    },
    "conflict": {
        "symfony/propel1-bridge": ">=2.8.0"
    },
    "replace": {
        "propel/propel-bundle": "1.5.*",
        "symfony/propel1-bridge": "<=2.8.0"
    }
}
