{
  "name": "dsdeboer/propel-bundle",
  "description": "Integration of Propel in Symfony2",
  "keywords": [
    "propel",
    "orm",
    "persistence"
  ],
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Duncan de Boer",
      "email": "duncan@charpand.nl"
    }
  ],
  "autoload": {
    "psr-4": {
      "Propel\\Bundle\\PropelBundle\\": ""
    },
    "files": [
      "autoload_aliases.php"
    ],
    "exclude-from-classmap": [
      "Tests/"
    ]
  },
  "require": {
    "dsdeboer/propel1": "^1.7.*",
    "symfony/framework-bundle": "^3.0.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8.21|^5.0.10",
    "phing/phing": "^2.12",
    "symfony/symfony": "^3.0.2",
    "sensio/framework-extra-bundle": "^3.0.2",
    "fzaninotto/faker": "^1.5"
  },
  "conflict": {
    "symfony/propel1-bridge": ">=2.8.0"
  },
  "replace": {
    "propel/propel-bundle": "*",
    "symfony/propel1-bridge": "<=2.8.0"
  }
}
